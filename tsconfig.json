{
    "compilerOptions": {
        "target": "es5",
      "module": "esnext",
      "lib": [
          "DOM",
          "DOM.Iterable",
          "ESNext"
      ],
      "esModuleInterop": true,
      "moduleResolution": "node",
      "jsx":"react",
      "rootDir": "src",
      "baseUrl": "src",
      "allowJs": true,
      "skipLibCheck": true,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true,
      "noEmit": true
    },
    "include": [
        "src"
    ]
}